@{
        ViewData["Title"] = "MyBlindTouch-game_index2";
}
@model MyBlindTouch.Models.GameModel
@using Microsoft.AspNetCore.Http;

<div class="banner">
    <h2>gametime=@ViewBag.gametime</h2>
    <h2>questiontime=@ViewBag.questiontime</h2>
    <h2>onoff=@ViewBag.soundonoff</h2>
    <h2>gamesound=@ViewBag.gamesound</h2>
    <table class="table">
    <thead>
        <tr>
            <th>
                @{
                    foreach (var item in Model.QuizStrCopy)
                    {
                        <h2>@item</h2>
                    }
                }
            </th>
            <th></th>
        </tr>
    </thead>
</table>
</div>