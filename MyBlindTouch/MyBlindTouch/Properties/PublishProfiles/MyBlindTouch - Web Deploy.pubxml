<Project>
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ResourceId>/subscriptions/0b6a2f56-6566-44ce-afc4-fd2fbf8ebafe/resourceGroups/MyBlindTouch_group/providers/Microsoft.Web/sites/MyBlindTouch</ResourceId>
    <ResourceGroup>MyBlindTouch_group</ResourceGroup>
